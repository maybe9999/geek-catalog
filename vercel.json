{
    "version": 2,
    "builds": [
      {
        "src": "index.html",
        "use": "@vercel/static"
      },
      {
        "src": "db.json",
        "use": "@vercel/node"
      },
      {
        "src": "assets/**/*",
        "use": "@vercel/static"
      },
      {
        "src": "js/**/*",
        "use": "@vercel/static"
      },
      {
        "src": "style.css",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/db.json"
      },
      {
        "src": "/assets/(.*)",
        "dest": "/assets/$1"
      },
      {
        "src": "/js/(.*)",
        "dest": "/js/$1"
      },
      {
        "src": "/style.css",
        "dest": "/style.css"
      }
    ]
  }